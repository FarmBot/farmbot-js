{
  "name": "farmbot",
  "version": "10.2.2",
  "description": "Farmbot Javascript client.",
  "scripts": {
    "build": "./build.sh",
    "typecheck": "node_modules/typescript/bin/tsc --noEmit",
    "coverage": "cat **/*lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test": "jest"
  },
  "author": "FarmBot.io, Rick Carlino",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.35",
    "coveralls": "^3.0.11",
    "typescript": "^3.8.3"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "mqtt": "^3.0.0",
    "ts-jest": "^24.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FarmBot/farmbot-js.git"
  },
  "keywords": [
    "farming",
    "agriculture",
    "iot",
    "arduino",
    "typescript"
  ],
  "jest": {
    "setupFiles": [],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageReporters": [
      "html",
      "json",
      "lcov"
    ]
  }
}
